---
import { Icon as AstroIcon } from "astro-icon/components";

interface Props {
  name: string;
  ariaLabel: string;
}

const { name, ariaLabel } = Astro.props;
---

<div class="flex size-10 items-center justify-center rounded-full shadow-down">
  <AstroIcon class="size-6" name={name} aria-label={ariaLabel} />
</div>
